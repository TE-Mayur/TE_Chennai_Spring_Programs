<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="testing" class="com.te.springcore.beans.Department">
	<property name="deptId" value="100"/>
	<property name="deptName" value="Testing" />
	</bean>
	
	<bean id="hr" class="com.te.springcore.beans.Department" >
	<property name="deptId" value="400"/>
	<property name="deptName" value="HR" />
	</bean>
	
	<bean id="dept" class="com.te.springcore.beans.Department" primary="true" >
	<property name="deptId" value="900"/>
	<property name="deptName" value="Dev" />
	</bean>


</beans>